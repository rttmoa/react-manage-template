{
  "name": "hooks-admin",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "hooks-admin open source management system",
  "author": {
    "name": "Hooks",
    "email": "848130454@qq.com",
    "url": "https://github.com/HalseySpicy"
  },
  "license": "MIT",
  "homepage": "https://github.com/HalseySpicy/Hooks-Admin",
  "repository": {
    "type": "git",
    "url": "git@github.com:HalseySpicy/Hooks-Admin.git"
  },
  "bugs": {
    "url": "https://github.com/HalseySpicy/Hooks-Admin/issues"
  },
  "scripts": {
    "start": "vite",
    "serve": "vite",
    "build:dev": "tsc && vite build --mode development",
    "build:test": "tsc && vite build --mode test",
    "build:pro": "tsc && vite build --mode production",
    "type:check": "tsc --noEmit --skipLibCheck",
    "preview": "npm run build:dev && vite preview",
    "lint:eslint": "eslint --fix --ext .js,.ts,.tsx ./src",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,ts,json,tsx,css,less,scss,html,md}\"",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:lint-staged": "lint-staged",
    "prepare": "husky install",
    "release": "standard-version",
    "commit": "git add -A && czg && git push"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.5",
    "@ant-design/pro-components": "^2.6.13",
    "@dnd-kit/core": "^6.0.8",
    "@dnd-kit/sortable": "^7.0.2",
    "@dnd-kit/utilities": "^3.2.1",
    "@reduxjs/toolkit": "^1.9.5",
    "ahooks": "^3.7.8",
    "antd": "^5.8.4",
    "axios": "^1.4.0",
    "dayjs": "^1.11.9",
    "echarts": "^5.4.3",
    "echarts-liquidfill": "^3.1.0",
    "i18next": "^23.4.5",
    "md5": "^2.3.0",
    "nprogress": "^0.2.0",
    "qs": "^6.11.2",
    "react": "^18.2.0",
    "react-colorful": "^5.6.1",
    "react-countup": "^6.4.2",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.2.0",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.15.0",
    "react-transition-group": "^4.4.5",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.0",
    "@commitlint/config-conventional": "^17.7.0",
    "@types/md5": "^2.3.2",
    "@types/node": "^20.5.4",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.7",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/react-transition-group": "^4.4.6",
    "@types/web-bluetooth": "^0.0.17",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vitejs/plugin-react": "^4.0.4",
    "autoprefixer": "^10.4.15",
    "cz-git": "^1.7.1",
    "czg": "^1.7.1",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "less": "^4.2.0",
    "lint-staged": "^14.0.1",
    "postcss": "^8.4.28",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "prettier": "^3.0.2",
    "rollup-plugin-visualizer": "^5.9.2",
    "stylelint": "^15.10.3",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-standard": "^34.0.0",
    "typescript": "^5.1.6",
